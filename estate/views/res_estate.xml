<odoo>
    <record id="view_users_form_inherit_property" model="ir.ui.view">
        <field name="name">res.users.form.inherit.property</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/> <!-- inherit existing Users form -->
        <field name="arch" type="xml">

            <!-- Add a new notebook page -->
            <notebook position="inside">
                <page string="Properties">
                    <field name="property_ids">
                        <tree string="Available Properties">
                            <field name="name"/>
                            <field name="expected_price"/>
                            <field name="state"/>
                        </tree>
                    </field>
                </page>
            </notebook>

        </field>
    </record>
</odoo>
